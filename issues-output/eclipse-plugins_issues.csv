```
<?xml version=""1.0"" encoding=""UTF-8"" standalone=""no""?>
<project>
  <configuration id=""ilg.gnuarmeclipse.managedbuild.cross.config.elf.debug.483978693"" name=""Debug"">
    <extension point=""org.eclipse.cdt.core.LanguageSettingsProvider"">
      <provider id=""ilg.gnuarmeclipse.managedbuild.cross.GCCBuiltinSpecsDetector"">
        <language id=""org.eclipse.cdt.core.gcc"">
          <entry kind=""includePath"" name=""/${ProjName}/#undef __thumb2__"">
